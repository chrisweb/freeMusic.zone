<tr data-fmz-trackId="<%= id %>" class="trackRow">
    <td class="trackImage">
        <% if (trackModel !== null) { %>
            <img src="<%= trackModel.get('jamendo_image') %>">
        <% } %>
    </td>
    
    <% if (this.options.context === 'twitterCharts') { %>
    
        <td class="position"><%- position %></td>
    
    <% } %>
    
    <td class="trackInformations">
        <% if (trackModel !== null) { %>
            <%- trackModel.get('jamendo_name') %> by <%- trackModel.get('jamendo_artist_name') %>
        <% } %>
    </td>
    <td class="trackActions">
        <button type="button" class="icon fa fa-play js-playTrack"></button>
        
		<% if (this.options.context !== 'collaborativePlaylistSearch') { %>
		
			<a href="https://twitter.com/share?url=http://jamen.do/t/<%- id %>&amp;via=freeMusicZone&amp;related=Jamendo&amp;hashtags=JAMENDO%2CFreeMusicZone%2CMUSIC&amp;text=<%= 'Discover ' + encodeURIComponent(trackModel.get('jamendo_name')) + ' on Jamendo Music' %>" type="button" class="icon fa fa-twitter js-shareOnTwitterTrack" rel="nofollow" target="_blank" style="target-new: tab;"></a>

			<% if (this.options.context !== 'twitterCharts') { %>

				<a href="https://www.facebook.com/sharer/sharer.php?u=" type="button" class="icon fa fa-facebook js-shareOnFacebookTrack" target="_blank" style="target-new: tab;"></a>
				<a href="https://plus.google.com/share?url=" type="button" class="icon fa fa-google-plus js-shareOnGooglePlusTrack" target="_blank" style="target-new: tab;"></a>
				<a href="" type="button" class="icon fa fa-weibo js-shareOnWeiboTrack" target="_blank" style="target-new: tab;"></a>
				<% /* icon does not yet exist in fa: fa-vk-ru https://github.com/FortAwesome/Font-Awesome/issues/3785 */ %>
				<a href="https://pinterest.com/pin/create/button/?url=&description=&media=" type="button" class="icon fa fa-facebook js-shareOnFacebookTrack" target="_blank" style="target-new: tab;"></a>
				<a href="http://www.reddit.com/submit?url=&title="></a>
				<a href="https://www.tumblr.com/widgets/share/tool?canonicalUrl={url}&title={title}&caption={desc}"></a>
				<% /* weibo? http://service.weibo.com/share/share.php?url=&appkey=&title=&pic=&ralateUid=&language=zh_cn */ %>
				<% /* tencent QQ? http://share.v.t.qq.com/index.php?c=share&a=index&url='+encodeURIComponent(url)+'&title='+encodeURIComponent(title)+'&appkey='+appkey */ %>
				<% /* whatsup? whatsapp://send?text= */ %>
				<% /* https://github.com/bradvin/social-share-urls */ %>

			<% } %>

		<% } %>

		<% if (this.options.context === 'collaborativePlaylistSearch') { %>

			<button type="button" class="icon fa fa-plus js-addTrackToCollaborative"></button>

		<% } %>

    </td>
        
</tr>